{
  "name": "smoking",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve demo",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-tailwind": "npx tailwindcss -c projects/ng-smoke/tailwind.config.js -i projects/ng-smoke/_index.scss -o projects/ng-smoke/ng-smoke.css",
    "watch-tailwind": "npx tailwindcss -c projects/ng-smoke/tailwind.config.js -i projects/ng-smoke/_index.scss -o projects/ng-smoke/ng-smoke.css --watch",
    "rolling-up": "npm run build-tailwind && ng build ng-smoke --configuration development",
    "rolling-up-prod": "ng build ng-smoke --prod",
    "catch-a-fire": "cd dist/ng-smoke && npm pack",
    "smoke": "cd dist/ng-smoke && npm publish",
    "rolling-and-smoke": "npm run rolling-up && npm run catch-a-fire"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.2.3",
    "@angular/cdk": "~14.2.2",
    "@angular/common": "~14.2.3",
    "@angular/compiler": "~14.2.3",
    "@angular/core": "~14.2.3",
    "@angular/forms": "~14.2.3",
    "@angular/platform-browser": "~14.2.3",
    "@angular/platform-browser-dynamic": "~14.2.3",
    "@angular/router": "~14.2.3",
    "rxjs": "~7.5.6",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.2.3",
    "@angular/cli": "~14.2.3",
    "@angular/compiler-cli": "~14.2.3",
    "@types/jasmine": "~3.10.6",
    "@types/node": "^18.7.18",
    "autoprefixer": "^10.4.12",
    "jasmine-core": "~4.4.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~4.0.2",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^14.2.1",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "typescript": "~4.8.3"
  }
}
